<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    <hibernate-mapping>
    <class name="com.pranav.demo.Team" table="team">
    <id name="teamId" column="teamid">
    <generator class="assigned"></generator>
    </id>
    <!-- This variable will match with primary key column of the table -->
    <!-- When generator class will be "assigned" which means the value of the primary 
    key will be provided by the developer database will not generate.-->
    
    <property name="teamName" column="teamname"></property>
   
    </class>
    </hibernate-mapping>